// ConsoleApplication33.cpp: определяет точку входа для консольного приложения.
//

#include "stdafx.h"
#include <string>
#include <iostream>
#include < math.h > 
using namespace std;


int _tmain(int argc, _TCHAR* argv[])
{
	int x;
	int y;
	int z;
	float s;
	cout << "x=";
	cin >> x;
	cout << "y=";
	cin >> y;
	cout << "z=";
	cin >> z;
	s = sqrt(x*x+y*y+z*z);
	cout << s;
	system("pause");
	return 0;
}
